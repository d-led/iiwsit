{
  "name": "iiwsit",
  "version": "1.0.0",
  "description": "Is It Worth Speeding It Up (Throughput)? - An interactive decision helper inspired by xkcd #1205",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:watch": "node scripts/build.mjs --watch",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "cy": "cypress open --e2e --browser chrome",
    "cypress": "cypress open --e2e --browser chrome",
    "cypress:headless": "cypress run",
    "e2e": "cypress run --e2e --browser chrome --spec 'cypress/e2e/!(screenshot).cy.ts'",
    "screenshots": "cypress run --e2e --browser chrome --spec 'cypress/e2e/*screenshot*.cy.ts'",
    "test:e2e": "npm run build && npm run e2e",
    "prettier": "prettier --write \"**/*.{js,ts,jsx,tsx,json,css,html,md}\"",
    "prettier:check": "prettier --check \"**/*.{js,ts,jsx,tsx,json,css,html,md}\"",
    "format": "npm run prettier"
  },
  "keywords": [
    "optimization",
    "calculator",
    "throughput",
    "architecture"
  ],
  "author": "",
  "license": "MPL-2.0",
  "dependencies": {
    "humanize-duration": "^3.33.1",
    "katex": "^0.16.23",
    "mermaid": "^11.12.0",
    "svg-pan-zoom": "^3.6.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@types/humanize-duration": "^3.27.4",
    "@types/node": "^20.19.21",
    "@vitest/ui": "^3.2.4",
    "cypress": "^15.4.0",
    "cypress-multi-reporters": "^2.0.5",
    "cypress-vite": "^1.8.0",
    "daisyui": "^5.2.3",
    "mocha-junit-reporter": "^2.2.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "serve": "^14.2.5",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "vitest": "^3.2.4"
  }
}
